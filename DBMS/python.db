USE Constrant;

CREATE TABLE Student
(
	 Roll CHAR(4) PRIMARY KEY,
    Name VARCHAR(50) NOT NULL,
    Email VARCHAR(60) UNIQUE,
    Address VARCHAR(255),
    Age INT
    CONSTRAINT checking_rule CHECK(Age>10)
);
 
